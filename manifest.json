{
    "name": "Notes Keeper",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to take notes",
    "icons":
    {
        "16": "icons/pen16.png",
        "32": "icons/pen32.png",
        "64": "icons/pen64.png",
        "128": "icons/pen128.png"
    },
    "action":
    {
        "default_popup": "main/index.html"
    },
    "permissions":
    [
        "downloads"
    ]
}